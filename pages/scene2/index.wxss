
.kivicube {
  display: block;
  width: 100vw;
  height: 100vh;
}
.test{
  position: absolute;
  width: 100%;
  height: 350rpx;
  z-index: 5;
  background: #Fff;
  top: 100rpx;
  left: 0;
}
.page{
  /* background: #202020; */
  /* background: rgba(255,255,255,.4); */
  color: #fff;
  text-align: center;
  position: absolute;
  z-index: 2;
  left: 0;
  width: 100%;
  bottom: 200rpx;
}
.content{
  width: calc(100% - 40rpx);
  margin: 0 auto;
}
.btn{
  background: #393958;
  color: #fff;
  margin: 0 10rpx;
  padding: 10rpx 14rpx;
  font-size: 24rpx;
  border-radius: 8rpx;
  display: inline-block;
  width: 150rpx;
  position: relative;
  z-index: 100;
}
.active{
  color: #01cc38;
}
.page .arrial-title{
  width: 450rpx;
  margin: 0rpx auto 40rpx;
  background: #393958;
  border-radius: 10rpx;
  text-align: center;
  font-size: 30rpx;
  padding: 10rpx 0;
  color: #fff;
}
.green-color{
  color: #01cc38;
}
.item{
  display: inline-block;
  font-size: 24rpx;
}
.status{
  width: 50rpx;
  height: 16rpx;
  border-radius: 4rpx;
  display: inline-block;
}
.status-text{
  display: inline-block;
  margin-right: 20rpx;
  margin-left: 20rpx;
}
.clear-margin{
  margin-right: 0;
}

.status-1{
  background:  #01cc38;
}
.status-2{
  background:  #ed9406;
}
.status-3{
  background:  #c72a16;
}
.status-4{
  background:  #150602;
}
.status-5{
  background:  #01cc38;
    border-radius: 50%;
  height: 16rpx;
  width: 16rpx;
}
.status-6{
  background:  #ddd;
  border-radius: 50%;
  height: 16rpx;
  width: 16rpx;
}
.status-7{
  background:  #ffed19;
  border-radius: 50%;
  height: 16rpx;
  width: 16rpx;
}
.subway-map{
  height: 640rpx;
  margin: 100rpx auto;
}
#myCanvas{
  width: 100%;
  height: 700rpx;
}
.subway-map{
  background: rgba(255,255,255,.8);
  width: calc(100% - 40rpx);
  margin:0 auto;
  border-radius: 20rpx;
  padding: 20rpx 0;
}
.camera-status{
  width: calc(100% - 40rpx);
  border-radius: 20rpx;
  box-sizing: border-box;
  color: #333;
  padding: 32rpx;
  background: rgba(255,255,255,.8);
  font-size: 28rpx;
  position: relative;
  text-align: left;
  margin:0 auto 40rpx;
}
.exit-icon{
  width: 36rpx;
  height: 36rpx;
  display: block;
  margin-left: 22rpx;
  margin-bottom: 6rpx;

}
.camera-icon{
  width: 56rpx;
  height: 56rpx;
  display: block;
  position: absolute;
  left: 32rpx;
  top: 38rpx;
  background: #ddd;
  border-radius: 50%;
  border: 10rpx solid #ddd;
}
.exit-btn{
  width: 80rpx;
  position: absolute;
  right: 12rpx;
  top: 42rpx;
  text-align: center;
  font-size: 24rpx;
}